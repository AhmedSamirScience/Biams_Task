<?xml version="1.0" encoding="utf-8"?>
<!--
      values/colors.xml

      (naming convention URLs)->
                               1- https://github.com/TreyCai/AndroidNamingConvention

      <color name="<THEME_NAME>_<COLOR_NAME>"><COLOR_VALUE></color>
      For example:
          <color name=”holo_blue_dark”>#ff0099cc</color>
          <color name=”material_blue_500”>#5677fc</color>

      If the color is not in the theme:
          <color name="<COLOR_NAME>"><COLOR_VALUE></color>

  -->
<resources>
    <color name="black">#FF000000</color>
    <color name="red">#FF0000</color>
    <color name="black_light">#BF000000</color>
    <color name="gray_light">#BFBFBFBF</color>
    <color name="gray_light_opacity_20">#33BFBFBF</color>
    <color name="gray_dark">#BFACACAC</color>
    <color name="gray_extra_light">#80EAEAEA</color>
    <color name="gray_medium_light">#80DFDEDE</color>
    <color name="white">#FFFFFF</color>
    <color name="gray">#FFCCCCCC</color>
    <color name="default_item_color">#FFFFFF</color> <!-- Default color -->
    <color name="selected_item_color">#BFBFBFBF</color> <!-- Selected color -->
</resources>